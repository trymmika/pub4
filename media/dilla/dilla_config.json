  "timing": {
    "dilla_swing": {
      "mpc_swing_range": [50, 75],
      "sweet_spot": [53, 56],
      "golden_ratio": 62.5,
      "ppqn": 96,
      "tempo_range": [82, 92],
      "nudge_ms": {
        "kick": -8,
        "snare": 12,
        "hihat": -3,
        "bass": -5
      },
      "humanize": {
        "velocity_variance": 15,
        "timing_variance_ms": 18
      }
    },
    "flylo_swing": {
      "tempo_range": [76, 110],
      "sweet_spot": [52, 58],
      "sidechain_pump": true
    },
    "madlib_swing": {
      "tempo_range": [88, 98],
      "approach": "live_triggering",
      "quantize": false,
      "note": "Madlib triggers everything live - no sequencing"
    }
  },

