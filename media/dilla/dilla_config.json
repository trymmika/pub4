  "effect_chains": {
    "_note": "Lo-fi effect chain configurations inspired by Sonitex STX-1260, NastyVCS, and vintage gear",
    
    "sonitex_stx1260": {
      "description": "Ultimate lo-fi workstation - 6 processing stages",
      "stages": {
        "mix": {
          "input_gain": { "min": -12, "max": 12, "default": 0 },
          "output_gain": { "min": -12, "max": 12, "default": 0 },
          "stereo_width": { "min": 0, "max": 200, "default": 100 },
          "mid_side_balance": { "min": -100, "max": 100, "default": 0 },
          "compressor": { "threshold": -20, "ratio": 4 }
        },
        "distortion": {
          "types": ["saturation", "distortion", "amplifier", "digital1", "digital2"],
          "drive": { "min": 0, "max": 100, "default": 20 },
          "pre_emphasis": true,
          "pre_filter_hz": { "min": 20, "max": 20000 },
          "mix": { "min": 0, "max": 100, "default": 50 }
        },
        "vinyl": {
          "pitch_warping": { "min": 0, "max": 100, "wow_rate_hz": 0.5 },
          "sibilance": { "min": 0, "max": 100, "note": "harmonic exciter for vinyl hiss quality" },
          "frequency_response": {
            "lowpass_hz": { "min": 1000, "max": 20000 },
            "highpass_hz": { "min": 20, "max": 500 }
          }
        },
        "tone": {
          "lowpass_hz": { "min": 500, "max": 20000 },
          "highpass_hz": { "min": 20, "max": 2000 },
          "resonance": { "min": 0, "max": 100 }
        },
        "noise": {
          "types": ["vinyl_pops", "vinyl_clicks", "vinyl_base", "tape_hiss", "mechanical", "custom_wav"],
          "pops_amount": { "min": 0, "max": 100 },
          "clicks_amount": { "min": 0, "max": 100 },
          "base_noise": { "min": 0, "max": 100 },
          "noise_gate": { "enabled": true, "threshold": -40 }
        },
        "sampling": {
          "bit_depth": { "min": 4, "max": 16, "default": 12 },
          "sample_rate": { "options": [44100, 22050, 11025, 8000], "default": 22050 },
          "reduction_types": ["linear", "sinc", "hold"],
          "anti_alias_filter": true,
          "dynamic_lowpass": {
            "cutoff_hz": { "min": 500, "max": 16000 },
            "resonance": { "min": 0, "max": 100 }
          },
          "presets": ["sp1200", "s950", "mirage", "lofi1", "lofi2", "extreme"]
        }
      },
      "modulation": {
        "lfo": { "rate_hz": { "min": 0.1, "max": 20 }, "shapes": ["sine", "triangle", "square", "random"] },
        "envelope_follower": { "attack_ms": 10, "release_ms": 100 }
      }
    },

    "nastyvcs": {
      "description": "Virtual console strip with transformer saturation",
      "stages": {
        "input": {
          "transformer": { "enabled": true, "saturation": 0.3 },
          "gain_db": { "min": -12, "max": 24 }
        },
        "saturation": {
          "amount": { "min": 0, "max": 100 },
          "type": "tube_transformer",
          "level_compensation": true
        },
        "filter": {
          "highpass_hz": { "min": 20, "max": 500 },
          "lowpass_hz": { "min": 1000, "max": 20000 }
        },
        "eq": {
          "air_shelf_hz": 12000,
          "air_gain_db": { "min": -6, "max": 6 },
          "boost_cut_low_hz": 100,
          "boost_cut_high_hz": 3000,
          "hi_q": false
        },
        "compressor": {
          "type": "opto_electric",
          "threshold_db": { "min": -40, "max": 0 },
          "ratio": { "min": 1, "max": 20 },
          "attack_modes": ["fast", "medium", "slow"],
          "release_modes": ["fast", "medium", "slow", "auto"]
        },
        "limiter": {
          "type": "brickwall_parallel",
          "modes": ["fast", "slow"],
          "wet_dry": { "min": 0, "max": 100 }
        },
        "output": {
          "transformer": { "enabled": true },
          "gain_db": { "min": -12, "max": 12 }
        }
      },
      "phase_alignment": { "enabled": true, "samples": { "min": -100, "max": 100 } }
    },

    "presets": {
      "dilla_donuts": {
