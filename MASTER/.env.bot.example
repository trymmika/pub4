# MASTER Bot Environment Variables
# Copy this file to .env and fill in your tokens

# OpenRouter API (Required for LLM)
OPENROUTER_API_KEY=your_openrouter_key_here

# Replicate API (Optional - for media generation)
REPLICATE_API_TOKEN=your_replicate_token_here

# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_PUBLIC_KEY=your_discord_public_key_here

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_WEBHOOK_SECRET=your_telegram_webhook_secret

# Slack Bot Configuration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your_slack_signing_secret

# Twitter/X Bot Configuration
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret
TWITTER_CONSUMER_KEY=your_twitter_consumer_key
TWITTER_CONSUMER_SECRET=your_twitter_consumer_secret

# Server Configuration (Optional)
PORT=8080
MASTER_TOKEN=your_custom_auth_token
